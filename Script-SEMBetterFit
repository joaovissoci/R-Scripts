# MODEL II

# Install the package 'sem'
# carry data library

library (sem) 	

# Change R's directory to that in which the data base .CSV file is 
# Load database

teste<- read.csv("BaseSEM.csv",header=T)
teste

# sem package model specification
mod.wh.1 <- specify.model()# Type these values that specify the model's relations (just use de Ctrl+R over each relation).
Support->SP,sp1,NA
Support->SM,NA,1
Rejection->RP,rp1,NA
Rejection->RM,NA,1
Overprot->OP,op1,NA
Overprot->OM,NA,1
MotI->MIAO,moti1,NA
MotI->MIEE,NA,1
MotI->MIC,moti2,NA
MotE->MERE,mote1,NA
MotE->MEIn,NA,1
MotE->MEId,mote2,NA
Support->MotI,SupMot,NA
Rejection->MotE,rej1,NA
Overprot->MotE,rej2,NA
MotI->SCI,misci1,NA
MotE->SCI,mesci1,NA
Support<->Support,dl1,NA
Rejection<->Rejection,dl2,NA
Overprot<->Overprot,dl3,NA
RM->SCI,rm1,NA
RM->SM,rm2,NA
RP->OP,rp2,NA
RP->SCI,rp3,NA
SP->RP,sp2,NA
SP->OP,sp3,NA
OP->RP,op1,NA
OP->SCI,op2,NA
OP<->OP,e1,NA
OM<->OM,e2,NA
SP<->SP,e3,NA
SM<->SM,e4,NA
RP<->RP,e5,NA
RM<->RM,e6,NA
MotI<->MotI,dl4,NA
MotE<->MotE,dl5,NA
MIAO<->MIAO,e7,NA
MIEE<->MIEE,e8,NA
MIC<->MIC,e9,NA
MEIn<->MEIn,e10,NA
MEId<->MEId,e11,NA
MERE<->MERE,e12,NA
SCI<->SCI,er1,NA
Support<->Rejection,suprej,NA
Support<->Overprot,supoover,NA
Rejection<->Overprot,rejover,NA
MotI->MotE,misci2,NA
MotE->MotI,mesci2,NA

# Insert de covariance matrix

cov.teste <- cov(teste, y = NULL, use = "everything", method = c("pearson", "kendall", "spearman"))
cov.teste

# Estimate de model (Here is where we have been finding difficulties)

sem.wh.1 <- sem(mod.wh.1, cov.teste, N=126)
summary(sem.wh.1)


# calcula índices de modificação (testes de pontuação) e alterações de parâmetros
# estimados para os parâmetros fixos e limitados em um modelo de equações estruturais
mod.indices(sem.wh.1)


#Better Fit, but with aliasing problems
